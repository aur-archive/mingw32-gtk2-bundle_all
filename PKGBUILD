package() {
	cd "${srcdir}"
	mkdir -p "${pkgdir}"/usr/i486-mingw32
	cp -rf bin/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf etc/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf include/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf lib/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf man/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf manifest/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf share/ "${pkgdir}"/usr/i486-mingw32/
	cp -rf src/ "${pkgdir}"/usr/i486-mingw32/
}
